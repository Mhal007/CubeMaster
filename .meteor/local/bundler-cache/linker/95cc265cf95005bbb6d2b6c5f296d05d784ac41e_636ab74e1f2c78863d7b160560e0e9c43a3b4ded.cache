 === 'OLL' || category === 'PLL' || category === '3x3x3') {\n      check(scramble, String);\n    }\n\n    if (category === 'OLL' || category === 'PLL') {\n      check(algorithmId, String);\n    } // Make sure the user is logged in before inserting a result\n\n    /* if (! Meteor.userId()) {\n     throw new Meteor.Error('not-authorized');\n     }*/\n\n\n    var doc = {\n      algorithmId: algorithmId,\n      category: category,\n      createdAt: new Date(),\n      real: real,\n      scramble: scramble,\n      time: time\n    };\n    Results.insert(doc);\n  },\n  'results.search': function (text) {\n    check(text, String);\n    return Results.find();\n  },\n  'results.remove': function (resultId) {\n    check(resultId, String);\n    /*const result = Results.findOne(resultId);\n     if (result.private && result.owner !== Meteor.userId()) {\n     // If the result is private, make sure only the owner can delete it\n     throw new Meteor.Error('not-authorized');\n     }*/\n\n    Results.remove(resultId);\n  }\n});\n//////////////////////////////////////////////////////////////////////////////////////////////////////\n\n}}},{\n  \"extensions\": [\n    \".js\",\n    \".json\",\n    \".html\",\n    \".ts\",\n    \".jsx\",\n    \".mjs\",\n    \".tsx\",\n    \".css\",\n    \".less\"\n  ]\n});\n\nrequire(\"/client/template.main.js\");\nrequire(\"/lib/global-helpers.js\");\nrequire(\"/client/utils.ts\");\nrequire(\"/collections/algorithms.ts\");\nrequire(\"/collections/results.ts\");\nrequire(\"/client/main.js\");","hash":"c3b29c120ae728cf9db2a7c32cccc6035991e633","servePath":"/app.js","sourceMap":{"version":3,"sources":["client/utils.ts","client/main.js","lib/global-helpers.js","collections/algorithms.ts","collections/results.ts"],"names":["React","module","link","v","Meteor","render","Container","startup","document","getElementById","export","getRandomScramble","allowedMoves","getRandomMove","Math","floor","random","length","movesConflict","moveA","moveB","split","some","char